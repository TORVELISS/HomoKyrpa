ChangeToFallenAngel.MouseButton1Down:connect(function()
_G.AutoLog = false;

spawn(function()
   while _G.AutoLog == true do
       wait()
local A_1 = "Ice Release"
local A_2 = "Ice Needles"
local A_3 = Vector3.new(-2989.5026855469, -99.391494750977, 5580.4506835938)
local Event = game:GetService("ReplicatedStorage").Remotes.KG
Event:FireServer(A_1, A_2, A_3)

local A_1 = "Jutsu"
local A_2 = "Lightning"
local A_3 = "Electromagnetic Murder"
local Event = game:GetService("ReplicatedStorage").Remotes.Jutsu
Event:FireServer(A_1, A_2, A_3)

end
end)

end)

